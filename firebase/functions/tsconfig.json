    "module": "commonjs",
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": false,
    "strict": true,
    "target": "es2017",
    "allowJs": true,
    "checkJs": false,
    "removeComments": true,
    "noImplicitAny": true
  },
  "compileOnSave": true,
  "include": [
    "../functions/src"
  ]
}
